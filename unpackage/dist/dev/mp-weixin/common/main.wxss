  /* 头部导航 */
.header{
  	background-color: #fff;
	border-radius: 0px 0px 30rpx 30rpx;
	padding-bottom: 20rpx;
	width: 100%;
	position: fixed;
	top: 0px;
	left: 0px;
	z-index: 999;
}
.headerTitle-name{
  	  color: #ffffff;
  	  font-size: 36rpx;
  	  font-weight: 400;
	  text-align: center;
	  z-index: 11;
}
.back-img{
	position: absolute;
	top: 0px;
	left: 0px;
	width: 100%;
	height: 100%;
	/* padding-top: var(--status-bar-height); */
}
.header{
  	width: 100%;
}
.headerBar{
}
.headerTitle{
  	padding: 0rpx 24rpx 0px 24rpx;
	position: relative;
}
.headerBack{
	width: 30rpx;
	height: 30rpx;
	display: block;
}
.title{
	width: 100%;
}
.channel{
	  width: 90%;
	  height: 50rpx;
	  position: absolute;
	  right: 0px;
	  overflow: hidden;
}
.img{
  		  width: 22rpx;
  		  height: 22rpx;
  		  margin: 0 8rpx;
}
.headerTitle-name2{
	  overflow: hidden;
	  color: @themeFontcolor;
	  font-size: 24rpx;
	  z-index: 11;
	  line-height: 36rpx;
}
.headerTitle-name2-txt{text-align: right;}
.headerTitle-name2-img{
	  width: 14rpx;
	  height: 8rpx;
	  margin-left: 7rpx;
}
.headerMenu{
  	margin-top: 47rpx;
	padding-left: 57rpx;
	padding-right: 50rpx;
	margin-bottom: 45rpx;
}
.headerMenu-list{
  	text-align: center;
  	color: #fff;
	opacity: .5;
  	font-size: 32rpx;
  	line-height: 32rpx;
	z-index: 11;
}
.headerMenu-list-active{
	 opacity: 1;
	  font-weight: 500;
	  z-index: 11;
}
.headerMenu-list:last-child{margin-right: 0px;}
.menu-data-box{position: relative;}
.menu-data{
	  width: calc(100% - 100rpx);
	  height: 48rpx;
	  padding-left: 57rpx;
	  overflow-y: hidden;
	  overflow-x: scroll;
	  white-space:nowrap;
	  scrollbar-width: none; /* firefox */
	  -ms-overflow-style: none; /* IE 10+ */
}
.menu-data::-webkit-scrollbar {
      display: none; /* Chrome Safari */
}
.data-list{
	  margin-right: 48rpx;
	  display: flex;
	  flex-direction: column;
	  justify-content: space-around;
}
.data-list-txt{
	font-size: 24rpx;
	line-height: 32rpx;
	height: 32rpx;
	opacity: .3;
	color: #fff;
}
.data-list-current{
	  width: 8rpx;
	  height: 8rpx;
	  background: @themeColor;
	  box-shadow: 0px -4rpx 8rpx 0px rgba(50,102,251,0.36);
	  border-radius: 4rpx;
	  margin: 9rpx auto 0px;
	  opacity: 0;
}
.data-list-active{
  	  color: @themeColor;
  	  opacity: 1;
}
.select-data{
	  position: absolute;
	  right: 0px;
	  top: 0px;
}
.select-linear-gradient{
	width: 72rpx;
	background: linear-gradient(90deg, #EEEEEE 0%, rgba(255,255,255,0) 0%, #fff 100%);
}
.select-right{
	  width: 72rpx;
	  font-size: 24rpx;
	  color: #fff;
	  padding-top: 4rpx;
}
.select-icon{
	width: 26rpx;
	height: 26rpx;
	line-height: 26rpx;
	margin-right: 9rpx;
}
.select-txt{
	height: 40rpx;
}
  /* 头部导航 end */
view {
  box-sizing: border-box;
}
@font-face {
  font-family: "Oswald Medium";
  src: url("https://www.ksjx365.com/uniapp_img/Barlowr/Barlow-SemiBold.ttf");
}
.font {
  font-family: "Oswald Medium", Arial, Helvetica, sans-serif;
  font-weight: 500;
}
.container {
  width: 100%;
}
.backImg {
  width: 100%;
  position: relative;
}
.disp-flex {
  display: flex;
}
.align-items {
  align-items: center;
}
.disp-justifyBetween {
  justify-content: space-between;
}
.disp-justifyAround {
  justify-content: space-around;
}
.flex-wrap {
  flex-wrap: wrap;
}
.flex {
  flex: 1;
}
.flex2 {
  flex: 2;
}
.color9 {
  color: #999999;
}
.color00C693 {
  color: #00c693;
}
.colorFF4C00 {
  color: #ff4c00;
}
.colorEF4034 {
  color: #ef4034;
}
.nowrapName {
  white-space: nowrap;
}
.ellipsis {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.empty {
  font-size: 28rpx;
  font-weight: 500;
  text-align: center;
  color: #3c61df;
  margin: 400rpx auto 0px;
}

