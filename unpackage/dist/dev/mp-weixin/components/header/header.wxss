.container{background: #EFF3FB;min-height: 100vh;margin-bottom: 32rpx;}
/* tab */
.tab-box{
	height: 100rpx;
	background-color: #fff;
}
.tab-left,.tab-right{
	height: 100rpx;
	background-color: #fff;
}
.tab-item{
	max-width: 220rpx;
	height: 60rpx;
	line-height: 60rpx;
	flex-direction: column;
	font-size: 28rpx;
	font-weight: 500;
	color: #929292;
	padding: 0px 24rpx;
}
.tab-item-name{
	width: 100%;
	text-align: center;
}
.tab-itemActive{color: #212121;}
.tab-item .line{
	width: 52rpx;
	height: 2rpx;
}
.tab-itemActive .line{background-color: #3C61DF;}
.title-data{
	font-size: 24rpx;
	margin-right: 20rpx;
}
.select-data{
	height: 60rpx;
}
.data-icon{
	width: 80rpx;
	height: 60rpx;
}
.data-txt{
	width: 120rpx;
	height: 60rpx;
	line-height: 60rpx;
	background: rgba(60, 97, 223, .1);
	font-size: 24rpx;
	color: #3C61DF;
	padding-right: 24rpx;
	text-align: center;
}
/* 列表 */
.list-box{
	margin: 0 24rpx;
}
.list{
	background: #fff;
	border-radius: 14rpx;
	padding: 42rpx 0px;
	margin-top: 20rpx;
}
.list-top{padding-left:24rpx;margin-bottom: 30rpx;}
.top-icon{
	width: 46rpx;
	height: 46rpx;
	background: #CACCD5;
	color: #FFFFFF;
	border-radius: 50%;
	text-align: center;
	font-size: 30rpx;
	margin-right: 25rpx;
}
.top-iconImg{
	width: 50rpx;
	height: 50rpx;
	margin-right: 25rpx;
}
.top-title{margin-bottom: 23rpx;}
.list-title{
	font-size: 28rpx;
	font-weight: 500;
	color: #212121;
	max-width: 450rpx;
}
.list-label{
	height: 32rpx;
	line-height: 32rpx;
	padding: 0px 8rpx;
	background: #EFF3FB;
	border-radius: 1rpx;
	font-size: 24rpx;
	color: #000E3C;
	opacity: .4;
	margin-left: 10rpx;
}
.progress{
	width: 517rpx;
	height: 8rpx;
	background-color: #F9F9F9;
}
.progress-num{
	font-size: 22rpx;
	color: #212121;
	font-weight: 400;
	margin-left: 14rpx;
}
.progress-bar{background-color: #3C61DF;height: 100%;}
.list-border{
	width: 100%;
	height: 1rpx;
	border-radius: 2rpx;
	border: 1rpx solid #EDEDED;
}
.list-bottom{margin-top: 36rpx;}
.list-bottom-name{
	font-size: 24rpx;
	line-height: 24rpx;
	font-weight: 400;
	color: #999999;
}
.list-bottom-num{
	font-size: 32rpx;
	line-height: 36rpx;
	color: #212121;
	margin-top: 17rpx;
}
.list-bottom-li:nth-child(1) .list-bottom-num{color: #EF4034;}
 .uni-load-more__text{font-size: 24rpx;margin: 0px;color: #3C61DF !important;}
 .uni-load-more__img{margin-right: 24rpx !important;}
.scroll-view-box {
	position: absolute;
	top: 0;
	right: 0;
	bottom: 0;
	left: 0;
}
.all-channels {
	padding-top: 24rpx;
	padding-bottom: 114rpx;
}
.close {
	width: 24rpx;
	height: 24rpx;
	border-radius: 4rpx;
	margin-left: 35rpx;
	margin-bottom: 27rpx;
}
.close image {
	width: 100%;
	height: 100%;
	display: block;
}
.channels-title {
	font-size: 28rpx;
	color: "#212121";
	margin-left: 33rpx;
	margin-bottom: 36rpx;
}
.channels-title-select {
	font-size: 24rpx;
	font-weight: 400;
	color: #212121;
	margin-right: 33rpx;
}
.channels-title-select image {
	width: 14rpx;
	height: 14rpx;
	margin-left: 11rpx;
}
.channels-list {
	width: 30.33%;
	height: 56rpx;
	line-height: 56rpx;
	border-radius: 14rpx;
	background-color: #F8F9FA;
	font-size: 24rpx;
	font-weight: 400;
	color: #212121;
	padding: 0px 20rpx;
	border: 1px solid transparent;
	margin-bottom: 20rpx;
	text-align: center;
	margin-left: 3%;
}
.channels-listActice {
	border-color: #3C61DF;
	color: #3C61DF;
}
.channels-listMarg {
	margin-right: 0px;
}
.channels-listWidth {
	width: 97%;
}
.channels-btn-list {
	width: 50%;
	height: 70rpx;
	line-height: 70rpx;
	text-align: center;
	font-size: 24rpx;
	font-weight: 400;
}
.channels-btn-list:nth-child(1) {
	color: #3C61DF;
	border-radius: 14rpx 0px 0px 14rpx;
	border: 1rpx solid #3C61DF;
}
.channels-btn-list:nth-child(2) {
	color: #fff;
	background-color: #3C61DF;
	border-radius: 0px 14rpx 14rpx 0px;
}
.channels-list-box {
	margin-bottom: 30rpx;
	width: 100%;
	padding-right: 4%;
	padding-left: 1%;
}
.channels-btn {
	padding: 24rpx 33rpx 50rpx;
	position: fixed;
	bottom: 0px;
	left: 0px;
	width: 100%;
	background-color: #fff;
	border-bottom-left-radius: 30rpx;
}
 .uni-drawer__content {
	border-radius: 30rpx 0px 0px 30rpx;
	width: 670rpx !important;
}
.close {
		margin-bottom: 0px;
}
.calendar-box {
		position: relative;
}
.calendar-title {
		font-size: 28rpx;
		font-weight: 500;
		text-align: center;
}
.triangle {
		position: relative;
}
.triangle image {
		width: 40rpx;
		height: 30rpx;
		display: block;
		margin-top: -1px;
		transition: all .5s ease;
}
.range {
		width: 50%;
		padding: 20rpx 0px 24rpx 30rpx;
}
.triangle-animation1 {
		-webkit-transform: translateX(140rpx);
		        transform: translateX(140rpx);
}
.triangle-animation2 {
		-webkit-transform: translateX(420rpx);
		        transform: translateX(420rpx);
}
.time-name {
		color: rgba(17, 26, 51, .6);
		margin-right: 20rpx;
}
.range-title {
		font-size: 24rpx;
		margin-bottom: 30rpx;
}
.time-list {
		font-size: 24rpx;
}
.time-list:first-child {
		margin-bottom: 30rpx;
}
.range-box {
		margin-top: 30rpx;
		padding-left: 40rpx;
		padding-right: 40rpx;
}
.range-active {
				border: 1px solid #0E34A0;
				border-radius: 14rpx;
				background-color: #F7F9FB;
}
.calendar-btn {
		padding: 24rpx 33rpx 50rpx;
		position: fixed;
		bottom: 0px;
		left: 0px;
		width: 100%;
		background-color: #fff;
		border-bottom-left-radius: 30rpx;
}
.calendar-btn-list {
		width: 50%;
		height: 70rpx;
		line-height: 70rpx;
		text-align: center;
		font-size: 24rpx;
		font-weight: 400;
}
.calendar-btn-list:nth-child(1) {
		color: #3C61DF;
		border-radius: 14rpx 0px 0px 14rpx;
		border: 1rpx solid #3C61DF;
}
.calendar-btn-list:nth-child(2) {
		color: #fff;
		background-color: #3C61DF;
		border-radius: 0px 14rpx 14rpx 0px;
}
 .uni-calendar__header {
	border: none;
}
 .uni-calendar__weeks-day {
	border: none;
	background-color: #F7F8FB;
	color: @themeFontcolor;
	font-size: 24rpx;
}
 .uni-calendar__weeks-day:first-child {
	color: @EyeCatchingColor;
}
 .uni-calendar__weeks-day:last-child {
	color: @EyeCatchingColor;
}
.mask-box {
	width: 100%;
	height: 100vh;
	background: rgba(0, 0, 0, 0.5);
	position: fixed;
	top: 0px;
	left: 0px;
	z-index: 999;
}
.bottom-popup {
	position: absolute;
	bottom: 0px;
	left: 0px;
	background-color: #fff;
	width: 100%;
	height: 663rpx;
	border-radius: 30rpx 30rpx 0px 0px;
	-webkit-animation-name: fade;
	        animation-name: fade;
	-webkit-animation-duration: .3s;
	        animation-duration: .3s;
	-webkit-animation-timing-function: linear;
	        animation-timing-function: linear;
}
@-webkit-keyframes fade {
0% {
		opacity: 0;
		-webkit-transform: translateY(100%);
		        transform: translateY(100%)
}
100% {
		opacity: 1;
		-webkit-transform: translateY(0);
		        transform: translateY(0)
}
}
@keyframes fade {
0% {
		opacity: 0;
		-webkit-transform: translateY(100%);
		        transform: translateY(100%)
}
100% {
		opacity: 1;
		-webkit-transform: translateY(0);
		        transform: translateY(0)
}
}
.picker-content {
	height: 500rpx;
	border-radius: 30rpx 30rpx 0px 0px;
	text-align: center;
}
 .uni-picker-view-indicator {
	height: 87rpx;
	line-height: 87rpx;
}
.picker-item {
	height: 87rpx;
	line-height: 87rpx;
	font-size: 34rpx;
	font-weight: 500;
}
.popup-btn {
	padding: 0px 34rpx;
}
.popup-btn-item {
		font-size: 32rpx;
		font-weight: 500;
		width: 100rpx;
		line-height: 100rpx;
		color: @themeFontcolor;
}
.determine {
		text-align: right;
		color: #0E34A0;
}
.comp-switch {
	position: absolute;
	top: 20rpx;
	left: -25rpx;
	color: #60D398;
	-webkit-transform: scale(0.5);
	        transform: scale(0.5);
	margin-left: -25rpx;
	z-index: 999;
	display: flex;
}
.comp-name {
	line-height: 64rpx;
	font-size: 36rpx !important;
	font-weight: 500 !important;
	color: @themeFontcolor !important;
}
.title-data{
	 color: #fff;
	 line-height: 47rpx;
}


